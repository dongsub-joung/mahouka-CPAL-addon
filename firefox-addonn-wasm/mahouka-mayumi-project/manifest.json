{
  "manifest_version": 2,
  "name": "mayumi",
  "version": "1.0",

  "description": "Sound booster.",

  "icons": {
    "48": "icons/mayumi.jpg"
 },
  "content_scripts": [
    {
      "matches": [ "*://*/*" ],
      "js": ["mayumi.js"]
    }
  ],
  "options_ui": {
	"page": "option.html"
  }, 
  "permissions": ["storage"]
}